<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="collectionexample" class="com.deloitte.CollectionExample">
    <property name="citylist">
    <list>
    <value>Bangalore</value>
    <value>Coimbatore</value>
    <value>Chennai</value>
    </list>
    </property>
    <property name="cityset">
    <set>
    <value>Bangalore</value>
     <value>Coimbatore</value>
    <value>Chennai</value>
    </set>
    </property>
    <property name="citymap">
    <map>
    <entry key="1" value="bangalore"></entry>
    <entry key="2" value="Coimbatore"></entry>
    <entry key="3" value="Hyderabad"></entry>
    </map>
    </property>
    <property name="cityprop">
    <props>
    <prop key="one">bangalore</prop>
    <prop key="two">Hyderabad</prop>
    <prop key="three">Mumbai</prop>
    </props>
    </property>
    </bean>
    </beans>